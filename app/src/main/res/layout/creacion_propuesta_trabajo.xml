<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Disponibilidad:"
        tools:layout_editor_absoluteX="195dp"
        tools:layout_editor_absoluteY="592dp" />

    <Spinner
        android:id="@+id/spinner6"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="194dp"
        tools:layout_editor_absoluteY="611dp" />

    <Spinner
        android:id="@+id/spinner5"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="611dp" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tipo de Servicio:"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="592dp" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descripción:"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="393dp" />

    <EditText
        android:id="@+id/editTextText15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="320dp" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Creación de Propuesta de Trabajo"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="93dp" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Titulo del Servicio:"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="177dp" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Precio Estimado:"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="273dp" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ubicación:"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="333dp" />

    <EditText
        android:id="@+id/editTextText13"
        android:layout_width="290dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="Ingrese aquí el titulo de su propuesta"
        android:inputType="text"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="201dp" />

    <EditText
        android:id="@+id/editTextText14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="S/.0.00"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="260dp" />

    <EditText
        android:id="@+id/editTextTextMultiLine"
        android:layout_width="300dp"
        android:layout_height="150dp"
        android:ems="10"
        android:gravity="start|top"
        android:hint="Ingrese aqui su descripción"
        android:inputType="textMultiLine"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="412dp" />

    <Button
        android:id="@+id/button10"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:text="Guardar Propuesta"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="667dp" />

    <Button
        android:id="@+id/button11"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:text="Cancelar"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="725dp" />

</androidx.constraintlayout.widget.ConstraintLayout>